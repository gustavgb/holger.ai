{
  "productName": "holger.ai",
  "version": "0.1.0",
  "identifier": "dk.gusgram.holger.ai",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "windows": [
      {
        "title": "holger.ai",
        "width": 1000,
        "height": 720,
        "minWidth": 480,
        "minHeight": 320,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false,
        "zoomHotkeysEnabled": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}